pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Bashing') {
            steps {
sh """
    echo 'foo'
    # bar
    echo 'hello'
"""
                
            }
        }
    }
}
