pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Bashing') {
            steps {
                powershell bash
                #!/bin/bash
                echo "hello world"
                
            }
        }
    }
}
